<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.thinkjoy.jx.k12system.dao.ex.IEXIdentityDAO">
    <delete id="delIdentityByCodes">
        DELETE  FROM k12system_identity WHERE identityCode in
        <foreach collection="identityCodes" item="item" index="index"
                 separator="," open="(" close=")">
            #{item}
        </foreach>
    </delete>
</mapper>