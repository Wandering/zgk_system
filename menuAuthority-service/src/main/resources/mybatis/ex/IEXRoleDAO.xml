<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.thinkjoy.zgk.zgksystem.dao.ex.IEXRoleDAO">
    <update id="updateRoleMenu">
            UPDATE k12system_role_menu set `status` = -1 WHERE roleCode = #{roleCode}
    </update>
    <update id="updateRoleResource">
        UPDATE k12system_role_resources set `status` = -1 WHERE roleCode = #{roleCode}
    </update>
</mapper>